<?xml version="1.0"?>
<object name="Atividade"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="plone" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Atividade projetizada</property>
 <property name="description" i18n:translate=""></property>
 <property name="icon_expr">string:${portal_url}/++resource++lx.demanda.imagens/atividade_icon.png</property>
 <property name="content_meta_type">Atividade</property>
 <property name="product">lx.demanda</property>
 <property name="factory">addAtividade</property>
 <property name="add_view_expr"></property>
 <property name="link_target"></property>
 <property name="immediate_view">base_view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="default_view">atividade</property>
 <property name="view_methods">
  <element value="atividade"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="atct_edit"/>
 <!--<alias from="edit" to="demanda_edit"/>-->
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object"
    condition_expr="not:object/@@plone_lock_info/is_locked_for_current_user|python:True"
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="History" action_id="history" category="object"
    condition_expr="" description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/atct_history" visible="False">
  <permission value="lx.demanda: View history"/>
 </action>
 <action title="External Edit" action_id="external_edit" category="object"
    condition_expr="object/externalEditorEnabled" description="" icon_expr=""
    link_target="" url_expr="string:${object_url}/external_edit"
    visible="False">
  <permission value="Modify portal content"/>
 </action>
</object>
